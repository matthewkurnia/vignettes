[application]

config/name="Spear"
run/main_scene="res://game/game.tscn"
boot_splash/image="res://wall/fill_white.png"
boot_splash/bg_color=Color( 1, 1, 1, 1 )
config/icon="res://icon.png"

[autoload]

Util="*res://utility/util.tscn"
Game="*res://game/game_singleton.gd"
Player="*res://player/player_singleton.tscn"
Cam="*res://camera/camera_singleton.gd"
Demo="*res://demo/demo.gd"
DialogicSingleton="*res://addons/dialogic/Other/DialogicSingleton.gd"

[display]

window/size/width=1920
window/size/height=1080
window/size/fullscreen=true
window/size/test_width=960
window/size/test_height=540
window/stretch/mode="2d"
window/stretch/aspect="keep"

[editor_plugins]

enabled=PoolStringArray( "res://addons/Trail/plugin.cfg", "res://addons/dialogic/plugin.cfg", "res://addons/object_placer/plugin.cfg", "res://addons/rmsmartshape/plugin.cfg" )

